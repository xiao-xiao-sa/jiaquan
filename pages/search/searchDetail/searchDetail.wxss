.search-container{
  width: 100%;
  height:80rpx; 
}
.search-input{
  background-color:#fff;
  border-radius: 30rpx;
  width: 700rpx;
  height: 60rpx;
  margin:10rpx auto;
  position: relative;
}
.search-input icon{
  position: absolute;
  left:20rpx;
  top:10rpx;
}
.search-input input{
  position: absolute;
  text-align: center;
  height:60rpx;
  width:600rpx;
  top:0;
  left:80rpx;
}

.shop-container{
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
} 
goods{
  margin-bottom:20rpx;
  margin-left:23rpx;
}
.no-shop{
  margin-top:200rpx;
  text-align: center;
}